[run]
omit =
    ./manage.py
    insights/*/migrations/*
    */tests/*
    */__init__.py
    Dockerfile
    */settings.py
    */apps.py
    insights/wsgi.py
    insights/event_driven/*
    insights/project/consumers/*
    */handle.py
    insights/asgi.py
    insights/conftest.py
    insights/authentication/authentication.py

[report]
fail_under = 70
skip_empty = true
show_missing=true
